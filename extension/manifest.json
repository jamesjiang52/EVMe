{
	"manifest_version": 3,
	"name": "EV Me",
	"version": "1.0",
	"description": "Suggest EV spreads.",
	"background": {
		"service_worker": "service-worker.js"
	},
	"content_scripts": [{
		"matches": ["*://play.pokemonshowdown.com/teambuilder"],
		"js": ["content.js"]
    }],
	"action": {
		"default_title": "EV Me"
	},
	"permissions": ["sidePanel", "tabs", "nativeMessaging"],
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Ctrl+B"
			}
		}
	}
}
